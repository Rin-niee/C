#include <iostream>
#include <stdio.h>
#include <math.h>
 
int main()
{
    double mas[3][3];
	int sum1, sum2;
    printf("%s", "Введите значения через пробел:");
    for (int i = 0; i<3; i++)
	{
    for (int j = 0; j<3; j++)
	{
    printf("mas[%d][%d]=",i,j);
    scanf("%d", &mas[i][j]);
    }
	}
    sum1=(int)(mas[0][0])+(int)(mas[1][1])+(int)(mas[2][2]);
    sum2=(int)(mas[2][0])+(int)(mas[1][1])+(int)(mas[0][2]);
    printf("%d %d", sum1, sum2);
    return 0;
    }
    //первое
    
    #include <iostream>
#include <stdio.h>
#include <math.h>
 
int main()
{
    int mas[2][2];
	int sum1, sum2;
    printf("%s", "Введите значения через пробел:");
    for (int i = 0; i<2; i++)
	{
    for (int j = 0; j<2; j++)
	{
    printf("mas[%d][%d]=",i,j);
    scanf("%d", &mas[i][j]);
    }
	}
    sum1=mas[0][0]+mas[1][1];
    sum2=mas[1][0]+mas[1][1];
    printf("%d %d", sum1, sum2);
    return 0;
    }
    
    //второе


#include <stdio.h>
#include <math.h>
#include <stdlib.h>
 
int main()
{
    double sum1=0,sum2=0;
	int i,j,n,o;
 	printf("Enter N:");
	scanf("%d", &n);   
	double mas[n][n];
    printf("%s", "Enter the values separated by a space: \n");
    for ( i = 0; i<n; i++)
	{
	    for ( j = 0; j<n; j++)
		{
	   	 	printf("mas[%d][%d]=",i,j);
	    	scanf("%lf", &mas[i][j]);
	    }
	}
	
    for ( i = 0; i<n; i++)
	{
	    for ( j = 0; j<n; j++)
		{
			sum2 = sum2+mas[i][n-1-j];
			if(i==j)
				{
				sum1 = sum1+mas[i][j];
				}
		}
	}
    printf("%lf\n %lf\n", sum1, sum2);
double kv[n][n];
for ( i = 0; i<n; i++)
{
	for ( j = 0; j<n; j++)
		{
			kv[i][j]=0;
			for ( o = 0; o<n; o++)
			{
				kv[i][j]=kv[i][j]+mas[i][o]*mas[o][j];
			}
		}
}
for ( i = 0; i<n; i++)
{
	for ( j = 0; j<n; j++)
		{
			printf("%lf", kv[i][j]);
		}
	printf("\n");
}
return 0;
}
//вариант для матрицы любого размера
