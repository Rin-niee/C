#include <stdio.h>
#include <stdlib.h>

typedef struct
{
	char lastname[60];
	char name[60];
	int birthyear;
	char gender[60];
	float height; 
} humen;

void sortarray(humen array[], int n, char a[])
 {
 	humen h;
 	int i, j, c;
 	c=0;
 	for (i=0; i<n; i++)
 	{
 		for (j=0; j<n; j++)
	 	{
	 		if (strcmp(a, "name")==0)
	 		{
	 			c= strcmp(array[i].name, array[j].name);
			}
			if (strcmp(a, "lastname")==0)
	 		{
	 			c= strcmp(array[i].lastname, array[j].lastname);
			}
			if (strcmp(a, "year")==0)
	 		{
	 			c = array[i].birthyear- array[j].birthyear;
			}
			if (strcmp(a, "gender")==0)
	 		{
	 			c= strcmp(array[i].gender, array[j].gender);
			}
			if (strcmp(a, "height")==0)
	 		{
	 			c = array[i].height - array[j].height;
			}
			if (c>0)
			{
				h = array[i];
				array[i]=array[j];
				array[j]=h;
			}
		}
	}
}
