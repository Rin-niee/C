  #include "8lab.h"

int main(int argc, char *argv[]) 
{	char a[100];
	int i,j,n;
	human* humens;
	FILE* f;
	f = fopen("text1.txt", "r");
	if (f == NULL)
	{
		printf("ERROR!");
	}
	else
	{ 	fscanf(f, "%d", &n);
		human* humens = (human*)malloc(n*sizeof(human));
		for (i = 0; i<n; i++)
		{
			fscanf(f, "%s %s %d %c %f\n", humens[i].lastname, humens[i].name, &humens[i].birthyear, &humens[i].gender, &humens[i].height);
		}
		fclose(f);
		printf("sort for(name, lastname, birthyear, gender, height):\n");
		scanf("%s ", &a);
		sortarray(humens, n, a[100]);
	}
	return 0;
}
