#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(int argc, char *argv[]) 
	{
		typedef struct
		{
			char name[50];
			char lastname[50];
			int birthyear;	
		} humen;
		humen mass1[4], mass1sort[4];
		int i;
		for (i=0; i<4; i++)
		{
			printf("Enter information about humans:");
			printf("Name: ");
			scanf("%s", mass1[i].name);
			printf("Last Name: ");
			scanf("%s", mass1[i].lastname);
			printf("Birth Year: ");
			scanf("%d", mass1[i].birthyear);
			
		}
		void sortbirthyear(humen mass1sort[4]);
		int j;
		humen h;
		for (j=0; j<4-1; j++)
		{
			if (mass1[j].birthyear> mass1[j+1].birthyear)
				{
					h = mass1[j];
					mass1[j]=mass1[j+1];
					mass1[j+1]=h;
				}
		}
		memcpy(mass1sort[4], mass1[4], sizeof(humen)*4);
		sortbirthyear(humen mass1sort[4]);
		for (i=0; i<4; i++)
		{
			printf("Name: %s/n", mass1sort[i].name);
			printf("Last Name: %s/n", mass1sort[i].lastname);
			printf("Birth Year: %d/n", mass1sort[i].birthyear);
		}
		return 0;
	}
