#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(int argc, char *argv[]) 
	{
		typedef struct
		{
			char name[50];
			char lastname[50];
			int birthyear;	
		} humen;
		humen mass1[4], mass1sort[4];
		int i;
		for (i=0; i<4; i++)
		{
			printf("Enter information about humans:");
			printf("Name: ");
			scanf("%s", mass1[i].name);
			printf("Last Name: ");
			scanf("%s", mass1[i].lastname);
			printf("Birth Year: ");
			scanf("%d", mass1[i].birthyear);
		}
		void sortarray(humen array[], int n)
		{	int j;
			for (i=0; i<n; i++)
			{
				for (j=0; j<n-i-1; i++)
				{
					if (mass1[j].birthyear> mass1[j+1].birthyear)
					{
						humen h;
						h = mass1[j];
						mass1[j]=mass1[j+1];
						mass1[j+1]=h;
					}
				}
			}
		}
		memmove(mass1sort, mass1, sizeof(humen)*4); #
		sortarray(mass1sort, 4);
		printf("sorted array:");
		for (i=0; i<4; i++)
		{
			printf("Name: %s\n", mass1sort[i].name);
			printf("Last Name: %s\n", mass1sort[i].lastname);
			printf("Birth Year: %d\n", mass1sort[i].birthyear);
		}
		return 0;
	}

