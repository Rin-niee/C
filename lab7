#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(int argc, char *argv[]) 
	{
		typedef struct
		{
			char name[50];
			char lastname[50];
			int birthyear;	
		} humen;
		humen mass1[4], mass1sort[4];
		int i;
		for (i=0; i<4; i++)
		{
			printf("Enter information about humans:", i+1);
			printf("Name: ");
			scanf("%s", mass1[i].name);
			printf("Last Name: ");
			scanf("%s", mass1[i].lastname);
			printf("Birth Year: ");
			scanf("%d", mass1[i].birthyear);
			
		}
		return 0;
	}
