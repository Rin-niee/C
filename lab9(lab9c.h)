#include <stdio.h> 
#include <stdlib.h> 
#include <string.h>

double schet(char *prim[1000]) 
{ 
	char *res[1000]; 
	double result, i;
	result = 0;
	res= strtok(prim[1000], " "); 
	while (res != NULL) 
	{ for (i=0; i<1000; i++)
	{
		if (res[i] == '+') 
		{ 
			result += res[i-1]+res[i+1]; 
		} 
		else if (res == '-') 
		{ 
			result -= res[i-1]+res[i+1]; 
		} 
	}
	return result; 
}
