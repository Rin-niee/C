#include <stdio.h>
#include <stdlib.h>

int main() 
{	int n,i,j;
	printf("Enter N: \n");
	scanf(" %d", &n);
	double a[n][n];
	double b[n][n];
	printf("%s", "Enter the values separated by a space for mass a: \n");
	for (i=0; i<n; i++)
	{
		for (j=0; j<n; j++)
		{
			printf("a[%d][%d]=",i,j);
    		scanf("%d", &a[i][j]);
		}
	}
	printf("%s", "Enter the values separated by a space for mass b: \n");
	for (i=0; i<n; i++)
	{
		for (j=0; j<n; j++)
		{
			printf("b[%d][%d]=",i,j);
    		scanf("%d", &b[i][j]);
		}
	}
	return 0;
}
