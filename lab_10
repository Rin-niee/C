#include <stdio.h>

#define MAX(x, y) ((x) > (y) ? (x) : (y))

int main() 
{
    int n, i;
    printf("Введите количество чисел: ");
    scanf("%d", &n);
    int nums[n];
    printf("Введите числа через пробел: ");
    for (i = 0; i < n; i++) {
        scanf("%d", &nums[i]);
    }
    int max = nums[0];
    for (i = 1; i < n; i++) {
       max = MAX(max, nums[i]);
    }
    printf("Максимальное число: %d\n", max);
    return 0;
}
