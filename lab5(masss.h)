#include <stdio.h>
#include <stdlib.h>
#include <math.h>

double sum(double *aa, double *bb, int n)
{
	double s,i,j;
	for(i=0; i<n; i++)
	{
		for(j=0, i<n, j++)
		{
			s = s+ aa[i][j]+bb[i][j];
		}
	}
	return s;
}
double diff(double *aa, double *bb)
{
	double d;
	d = *aa-*bb;
	return d;
}
double prod(double *aa, double *bb)
{
	double p;
	p = *aa*(*bb;
	return p;
}
